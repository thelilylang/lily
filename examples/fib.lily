fun fib(n Int32) Int32 =
	if n < 0 do
		return -1;
	elif n == 0 do
		return 0;
	elif n == 1 or n == 2 do
		return 1;
	else
		return fib(n - 1) + fib(n - 2);
	end
end

fun main Int32 =
	val res := fib(40);
	val res2 := fib(20) + res;

